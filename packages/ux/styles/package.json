{
    "name": "@mitchell/ux.styles",
    "version": "1.0.4",
    "description": "I am a bunch of styles!",
    "main": "dist/index.js",
    "module": "dist/index.es.js",
    "types": "dist/src/public_apis.d.ts",
    "scripts": {
        "build": "npm run export",
        "build-copy": "copyfiles src/**/*.{tsx,ts,js,jsx} dist",
        "export": "npm run clean && npm run export-js && npm run build-copy",
        "export-js": "bili --format es,umd,umd-min --inline 0 --input src/public_apis.tsx",
        "clean": "rimraf dist",
        "serve": "num run start",
        "pretty": "prettier --write src/**/*.{tsx,ts}",
        "lint": "tslint src/**/*.{tsx,ts}",
        "lint-fix": "tslint --fix src/**/*.{tsx,ts}"
    },
    "dependencies": {
        "@material-ui/core": "~1.0.0",
        "@material-ui/icons": "~1.0.0",
        "@mitchell/estimating.parts.categories": "1.0.4",
        "@mitchell/ux.contracts": "1.0.4",
        "emotion": "~9.1.3",
        "i18next": "~11.3.2",
        "mi.ux-uistyles": "~2.2.57",
        "mi.ux.styles": "~1.0.4",
        "prop-types": "~15.6.1",
        "react": "^16.3.2",
        "react-dom": "^16.3.2",
        "react-emotion": "~9.1.3",
        "react-i18next": "~7.6.1"
    }
}
